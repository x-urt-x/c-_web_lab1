@model List<Person>
@{
    Layout = "_Layout";
    ViewBag.Title = "List"; 
}

    <h1 style="text-align: center">Review List</h1>
    <table border="1"  class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Date</th>
                <th>Name</th>
                <th>Mark</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var person in Model)
        {
                    <tr>
                        <td>@person.Date</td>
                        <td>@person.Name</td>
                        <td>@person.Mark</td>
                    </tr>
        }
        </tbody>
    </table>
